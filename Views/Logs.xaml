<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:viewmodels="clr-namespace:GonzalezFabioExamen3P.ViewModels"
             x:Class="GonzalezFabioExamen3P.Views.Logs"
             Title="Logs"
             BindingContext="{Binding LogsViewModel, Source={x:Static viewmodels:LogsViewModel}}">

    <CollectionView ItemsSource="{Binding Logs}">
        <CollectionView.ItemTemplate>
            <DataTemplate>
                <Label Text="{Binding .}" Padding="10"/>
            </DataTemplate>
        </CollectionView.ItemTemplate>
    </CollectionView>

</ContentPage>
